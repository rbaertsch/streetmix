    "gulp-sourcemaps": "latest",
    "gulp-tap": "0.1.3",
    "handjs": "1.3.11",
    "i18next": "latest",
    "i18next-xhr-backend": "latest",
    "jade": "1.11.0",
    "js-cookie": "2.1.3",
